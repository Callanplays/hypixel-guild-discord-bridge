{
  "name": "hypixel-guild-discord-bridge",
  "version": "2.0.0",
  "description": "A Hypixel guild chat and Discord chat bridge",
  "main": "index.ts",
  "scripts": {
    "start": "node --require ts-node/register index.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test:compile": "npx tsc --noEmit",
    "test:run": "node --require ts-node/register index.ts test-run",
    "validate": "npx prettier . --write --list-different && npx eslint . --fix && npx tsc --noEmit && npm run test:run"
  },
  "license": "MIT",
  "keywords": [
    "hypixel",
    "discord",
    "chat",
    "bridge"
  ],
  "author": "aidn5 (aidn.david3@gmail.com)",
  "dependencies": {
    "@hkh12/node-calc": "^4.2.1",
    "axios": "^1.6.0",
    "bad-words": "^3.0.4",
    "clean": "^4.0.2",
    "discord.js": "^14.7.1",
    "emoji-name-map": "^1.2.9",
    "hypixel-api-reborn": "^10.0.0",
    "log4js": "^6.4.2",
    "minecraft-data": "^3.61.0",
    "minecraft-protocol": "^1.46.0",
    "moment": "^2.29.4",
    "node-cache": "^5.1.2",
    "prismarine-chat": "^1.9.1",
    "prismarine-registry": "^1.7.0",
    "prom-client": "^14.0.1",
    "skyhelper-networth": "^1.3.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.1",
    "socks": "^2.7.1",
    "tiny-typed-emitter": "^2.1.0",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^43.0.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^48.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "prismarine-nbt": "^2.5.0"
  }
}
